{
  "name": "r6maps",
  "//": "WHEN UPDATING VERSION #: Search and replace # packag.json and index.html of site.  CSS now has a separate version #",
  "//": "(this is hacky for now - did this because # doesn't increment often and avoided some complexities)",
  "version": "1.2.4",
  "description": "Rainbow Six Siege map quick references",
  "watch": {
    "build:js": "dev/js/*.js"
  },
  "scripts": {
    "test:js": "eslint dev/js/*.js dev/utils/*.js",
    "test": "npm run test:js",
    "build:aboutcss": "rimraf ./site/css/about* && scss './dev/scss/about.scss':'./site/css/about.1.1.0.css'",
    "build:css": "rimraf ./site/css/main* && scss './dev/scss/main.scss':'./site/css/main.1.2.4.css'",
    "watch:css": "rimraf ./site/css/main* && scss --watch './dev/scss/main.scss':'./site/css/main.1.2.4.css'",
    "clean:js": "rimraf ./site/js/*",
    "build:js": "npm run clean:js && uglifyjs --compress --mangle -o ./site/js/r6-maps.1.2.4.min.js --source-map  ./site/js/r6-maps.1.2.4.min.js.map --source-map-url r6-maps.1.2.4.min.js.map --source-map-include-sources -- ./dev/js/langs/r6-maps.lang-terms.js ./dev/js/langs/r6-maps.lang-terms.*.js ./dev/js/r6-maps.map-data.js ./dev/js/r6-maps.map-render.js ./dev/js/r6-maps.controls.js ./dev/js/r6-maps.select-maps.js ./dev/js/r6-maps.sessions.js ./dev/js/r6-maps.main.js",
    "watch:js": "npm run clean:js && npm-watch",
    "build": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capajon/r6maps.git"
  },
  "keywords": [
    "Rainbow",
    "Six",
    "Maps",
    "Siege"
  ],
  "author": "Jon Capa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capajon/r6maps/issues"
  },
  "homepage": "https://github.com/capajon/r6maps#readme",
  "devDependencies": {
    "eslint": "^2.2.0",
    "npm-watch": "^0.1.7",
    "rimraf": "^2.5.4"
  }
}
